{
    "name": "MST3K Tube",
    "version": "1.0",
    "description": "MST3K on YouTube",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["https://*.youtube.com/*", "https://blog.lateral.io/*"],
            "all_frames": true,
            "css": ["myStyles.css"],
            "js": ["contentScript.js"]
        }
    ],
    "web_accessible_resources": ["mst3k_overlay.svg"]
}
